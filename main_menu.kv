MDScreen:
    name:"main_menu"
    MDFloatLayout:
        md_bg_color: (33/255, 150/255, 243/255, 1)
    MDRectangleFlatIconButton:
        icon: "heart_made_by_me.png"
        font_size: 20
        text_padding: "30dp"
        text: "My health"
        size_hint_x: .5
        size_hint_y: 0.2
        theme_text_color: "Custom"
        text_color: "white"
        line_color: "gray"
        md_bg_color: (33/255, 33/255, 33/255, 1)
        pos_hint: {'center_x': 0.25, 'center_y':0.5}
        theme_icon_color: "Custom"
        icon_color: "orange"
        on_press:
            root.manager.transition.direction = "left"
            root.manager.current = "stats"
